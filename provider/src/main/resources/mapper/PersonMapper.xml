<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nanyin.provider.dao.PersonMapper">

    <select id="findPersonByName" parameterType="String" resultType="com.nanyin.provider.entity.Person">
        SELECT * FROM person WHERE name = #{name}
    </select>
</mapper>